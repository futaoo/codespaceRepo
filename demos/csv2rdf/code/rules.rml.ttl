@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix ex: <http://www.example.com/>.

:map_station_0 rml:logicalSource :source_0.
:source_0 a rml:LogicalSource;
    rml:source "../datasets/dublinRecord.csv";
    rml:referenceFormulation ql:CSV.
:map_station_0 a rr:TriplesMap;
    rdfs:label "station".
:s_0 a rr:SubjectMap.
:map_station_0 rr:subjectMap :s_0.
:s_0 rr:template "http://example.com/Station/{STATION}".
:pom_0 a rr:PredicateObjectMap.
:map_station_0 rr:predicateObjectMap :pom_0.
:pm_0 a rr:PredicateMap.
:pom_0 rr:predicateMap :pm_0.
:pm_0 rr:constant rdf:type.
:pom_0 rr:objectMap :om_0.
:om_0 a rr:ObjectMap;
    rr:constant "http://www.example.com/Station";
    rr:termType rr:IRI.
:pom_1 a rr:PredicateObjectMap.
:map_station_0 rr:predicateObjectMap :pom_1.
:pm_1 a rr:PredicateMap.
:pom_1 rr:predicateMap :pm_1.
:pm_1 rr:constant <http://www.example.com/Property/name>.
:pom_1 rr:objectMap :om_1.
:om_1 a rr:ObjectMap;
    rml:reference "NAME";
    rr:termType rr:Literal.
